@page
@model RecepcionistaDeleteModel

<h1>Excluir Recepcionista</h1>

<p>Tem certeza de que deseja excluir este recepcionista?</p>

<form method="post">
    <input type="hidden" asp-for="IdRecepcionista" />
    <button type="submit" class="btn btn-danger">Confirmar Exclusão</button>
    <a asp-page="Recepcionistas" class="btn btn-secondary">Cancelar</a>
</form>
