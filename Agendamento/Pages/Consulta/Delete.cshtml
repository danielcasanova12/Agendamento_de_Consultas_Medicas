@page
@model ConsultaDeleteModel

<h1>Excluir Consulta</h1>

<p>Tem certeza de que deseja excluir esta consulta?</p>

<form method="post">
    <input type="hidden" asp-for="IdConsulta" />
    <button type="submit" class="btn btn-danger">Confirmar Exclusão</button>
    <a asp-page="Index" class="btn btn-secondary">Cancelar</a>
</form>
